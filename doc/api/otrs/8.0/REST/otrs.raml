                        X-OTRS-API-Debug-SQLTrace: 
                            description: Includes SQL debugging information, if 'X-OTRS-API-Debug' was sent and 'enable_debugger_sql_trace' is enabled.
                            required: false
                        X-OTRS-API-Debug-STDERRLog: 
                            description: Includes STDERR debugging information, if 'X-OTRS-API-Debug' was sent and 'enable_debugger_stderr_log' is enabled.
                            required: false
                        X-OTRS-API-Version: 
                            description: Provides endpoint version information.
                            example: 1.0.0
                            required: true
                422: 
                    body: 
                        application/json: 
                            example: {"Errors":{"Field1":{"Message":"..."}}}
                    description: Invalid input data.
                    headers: 
                        X-OTRS-API-Debug-SQLTrace: 
