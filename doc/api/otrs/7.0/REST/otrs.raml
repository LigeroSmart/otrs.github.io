                            required: false
                        X-OTRS-API-Version: 
                            description: Provides endpoint version information.
                            example: 1.0.0
                            required: true
                422: 
                    body: 
                        application/json: 
                            example: {"Errors":{"Field1":{"Message":"..."}}}
                    description: Invalid input data.
                    headers: 
                        X-OTRS-API-Debug-SQLTrace: 
                            description: Includes SQL debugging information, if 'X-OTRS-API-Debug' was sent and 'enable_debugger_sql_trace' is enabled.
                            required: false
                        X-OTRS-API-Debug-STDERRLog: 
                            description: Includes STDERR debugging information, if 'X-OTRS-API-Debug' was sent and 'enable_debugger_stderr_log' is enabled.
                            required: false
                        X-OTRS-API-Version: 
                            description: Provides endpoint version information.
                            example: 1.0.0
                            required: true
                900: 
                    body: 
                        application/json: 
                            example: [{"ErrorIdentifier":"EmailSendingFailed","ErrorMessage":"Could not send password recovery instructions via email. Please contact the administrator."},{"ErrorMessage":"Multiple accounts with the same email address were found. Please contact the administrator.","ErrorIdentifier":"MultipleAccountsFound"}]
                    description: Custom error response, see example for a list of possible responses.
                    headers: 
                        X-OTRS-API-Debug-SQLTrace: 
                            description: Includes SQL debugging information, if 'X-OTRS-API-Debug' was sent and 'enable_debugger_sql_trace' is enabled.
                            required: false
                        X-OTRS-API-Debug-STDERRLog: 
                            description: Includes STDERR debugging information, if 'X-OTRS-API-Debug' was sent and 'enable_debugger_stderr_log' is enabled.
                            required: false
                        X-OTRS-API-Version: 
                            description: Provides endpoint version information.
                            example: 1.0.0
                            required: true
